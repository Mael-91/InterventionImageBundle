{
  "name": "mael/intervention-image-bundle",
  "description": "Simple intervention/image integration for symfony projects",
  "keywords": ["symfony", "intervention", "image", "image manipulation", "gd", "imagick", "watermark", "thumbnail"],
  "version": "1.0.0",
  "license": "MIT",
  "type": "symfony-bundle",
  "homepage": "https://github.com/Mael-91",
  "authors": [
    {
      "name": "Mael",
      "homepage": "https://mael-91.me/"
    },
    {
      "name": "Community Contributions",
      "homepage": "https://github.com/Mael-91/InterventionImageBundle/graphs/contributors"
    }
  ],
  "require": {
    "php": "^7.1",
    "symfony/config": "^4.4|^5.0",
    "symfony/dependency-injection": "^4.4|^5.0",
    "symfony/http-kernel": "^4.4|^5.0",
    "intervention/image": "^2.5"
  },
  "suggest": {
    "ext-gd": "to use GD library based image processing.",
    "ext-imagick": "to use Imagick based image processing."
  },
  "autoload": {
    "psr-4": {
      "Mael\\InterventionImageBundle\\": "src/"
    }
  }
}
